
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.musicproj.menu.MainActivity">



    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:minHeight="?attr/actionBarSize"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        app:theme="@style/ThemeOverlay.AppCompat.Dark"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintHorizontal_bias="0.466" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="122dp"
        android:layout_height="0dp"
        android:layout_marginEnd="53dp"
        android:contextClickable="false"
        android:cursorVisible="false"
        android:editable="false"
        android:ems="10"
        android:inputType="none|textPersonName"
        android:text="Shuffle"
        android:textColor="@android:color/background_light"
        android:textSize="10dp"
        tools:layout_constraintRight_creator="1"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:background="#00000000"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        app:layout_constraintRight_toRightOf="@+id/editText3"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toTopOf="@+id/textView"
        android:layout_marginRight="59dp"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="122dp"
        android:layout_height="51dp"
        android:layout_marginEnd="8dp"
        android:editable="false"
        android:ems="10"
        android:inputType="none|textPersonName"
        android:text="Exit"
        android:textColor="@android:color/white"
        android:textColorLink="@android:color/background_light"
        android:textSize="10dp"
        tools:layout_constraintRight_creator="1"
        tools:text="Exit"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:background="#00000000"
        tools:layout_constraintBaseline_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView"
        android:layout_marginRight="8dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="59dp"
        android:layout_height="0dp"
        android:editable="false"
        android:ems="10"
        android:inputType="none|textPersonName"
        android:text="Import"
        android:textColor="@android:color/white"
        android:textSize="10dp"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:text="import"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:background="#00000000"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        app:layout_constraintRight_toRightOf="@+id/editText2"
        app:layout_constraintLeft_toRightOf="@+id/editText2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:editable="false"
        android:inputType="none"
        android:text="Settings"
        android:textColor="@android:color/white"
        android:textSize="10dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:text="Settings"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:background="#00000000"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText4"
        tools:layout_constraintBaseline_creator="1" />

    <ListView
        android:id="@+id/song_list"
        android:layout_width="0dp"
        android:layout_height="0dp"


        android:layout_marginBottom="2dp"
        android:layout_marginEnd="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginStart="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

    </ListView>


</android.support.constraint.ConstraintLayout>


